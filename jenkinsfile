@Library('jenkins-shared-library@master') _
import SonarScanner
import jenkins.model.*

pipeline 
{
  agent any 
  stages 
  {
    stage ('SonarQube Analysis') 
      {
       steps 
         {
           script
              {
              jenkins = Jenkins.instance
              def sc = SonarScanner.runSonarScanner();
              }
            }
         }
    }
  }
  
