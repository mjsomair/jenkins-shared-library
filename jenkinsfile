@Library('jenkins-shared-library@master') _
import SonarScanner
pipeline 
{
  agent any 
  stages 
  {
    stage ('SonarQube Analysis') 
      {
       steps 
         {
           script
              {
              SonarScanner sonarScanner = new SonarScanner();
              sonarScanner.runSonarScanner();
              }
            }
         }
    }
  }
  
